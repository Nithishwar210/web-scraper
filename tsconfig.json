{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "allowUnusedLabels": false,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noUnusedLocals": false,
        "noUnusedParameters": true,
        "strictNullChecks": true
    },
    "lib": ["es2015"]
}
